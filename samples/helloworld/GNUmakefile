### Variables: ###

HRBFLAGS = -go -n -a -v -m -es2 -w2
HRBINCLUDES = -I../../include/ -I/usr/include/harbour/
CC = hblnk
BUILDDIR = obj/gcc
LDFLAGS = -L../../obj/gcc/
HELLOWORLD_OBJECTS =  \
	$(BUILDDIR)/helloworld_helloworld.o

### Conditionally set variables: ###



all: $(BUILDDIR)
$(BUILDDIR):
	@mkdir -p $(BUILDDIR)

### Targets: ###

all: ./helloworld

install: all

uninstall: 

clean: 
	rm -f $(BUILDDIR)/*.o
	rm -f $(BUILDDIR)/*.d
	rm -f ./helloworld

./helloworld: $(HELLOWORLD_OBJECTS)
	$(CC) -o $@ $(HELLOWORLD_OBJECTS) $(LDFLAGS)  -lwxharbour `wx-config --libs`

$(BUILDDIR)/helloworld_helloworld.o: ./helloworld.prg
	harbour $(HRBFLAGS) $(HRBINCLUDES) -o$@ $<

.PHONY: all install uninstall clean


# Dependencies tracking:
-include $(BUILDDIR)/*.d
